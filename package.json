{
  "name": "@lenne.tech/gluegun-menu",
  "version": "0.0.4",
  "description": "Menu for gluegun CLIs",
  "keywords": [
    "gluegun",
    "menu",
    "cli",
    "extension",
    "lenne.tech"
  ],
  "contributors": [
    "Pascal Klesse",
    "Kai Haase <kai.haase@lenne.tech> (http://lenne.tech)",
    "Angel Rodr√≠guez <angelmanuelrs@gmail.com> (https://angel-rs.netlify.com)"
  ],
  "homepage": "http://lenne.tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lenneTech/gluegun-menu"
  },
  "bugs": {
    "url": "https://github.com/lenneTech/gluegun-menu/issues"
  },
  "scripts": {
    "build": "npm run lint && npm run clean-build && npm run compile",
    "clean-build": "rimraf ./dist",
    "compile": "tsc -p .",
    "coverage": "jest --coverage",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json}'",
    "lint": "eslint --ext .js,.ts src",
    "prepack": "npm run build",
    "preversion": "npm run lint",
    "test": "jest",
    "watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@lenne.tech/npm-package-helper": "^0.0.10",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.30",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "gluegun": "^4.0.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "gluegun": ">=3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ts-node --skip-project extras/sync-version.ts && pretty-quick --staged",
      "pre-push": "npm run lint"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "LICENSE",
    "assets/demo.gif",
    "README.md"
  ]
}
