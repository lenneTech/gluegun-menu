{
  "name": "@lenne.tech/gluegun-menu",
  "version": "0.0.7",
  "description": "Menu for gluegun CLIs",
  "keywords": [
    "gluegun",
    "menu",
    "cli",
    "extension",
    "lenne.tech"
  ],
  "contributors": [
    "Pascal Klesse",
    "Kai Haase <kai.haase@lenne.tech> (http://lenne.tech)",
    "Angel Rodr√≠guez <angelmanuelrs@gmail.com> (https://angel-rs.netlify.com)"
  ],
  "homepage": "http://lenne.tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lenneTech/gluegun-menu"
  },
  "bugs": {
    "url": "https://github.com/lenneTech/gluegun-menu/issues"
  },
  "scripts": {
    "build": "npm run lint && npm run clean-build && npm run compile",
    "clean-build": "rimraf ./dist",
    "compile": "tsc -p .",
    "coverage": "jest --coverage",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json}'",
    "format:fix": "pretty-quick --staged",
    "format:fixAll": "prettier --write src",
    "lint": "eslint './src/**/*.{ts,js,vue}'",
    "lint:fix": "eslint './src/**/*.{ts,js,vue}' --fix",
    "prepack": "npm run build",
    "preversion": "npm run lint",
    "test": "jest",
    "watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@lenne.tech/eslint-config-ts": "2.0.1",
    "@lenne.tech/npm-package-helper": "0.0.12",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.17",
    "@typescript-eslint/eslint-plugin": "8.32.0",
    "@typescript-eslint/parser": "8.32.0",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.5",
    "gluegun": "5.2.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "pretty-quick": "4.1.1",
    "rimraf": "6.0.1",
    "ts-jest": "29.3.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "gluegun": ">=3"
  },
  "overrides": {
    "apisauce@*": "3.1.1",
    "cross-spawn@*": "7.0.6",
    "ejs@*": "3.1.10",
    "semver@*": "7.7.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "LICENSE",
    "assets/demo.gif",
    "README.md"
  ]
}
